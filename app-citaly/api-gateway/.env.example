# Variables de entorno para Google Calendar

# Backend - API Gateway
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3001/api/google-calendar/callback

# MySQL Database
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=
DB_NAME=citaly_db
DB_PORT=3306

# JWT
JWT_SECRET=your_jwt_secret_key_here

# Server
PORT=3001
NODE_ENV=development

# Webhook para notificaciones de Google Calendar
GOOGLE_WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_BASE_URL=http://localhost:3001
