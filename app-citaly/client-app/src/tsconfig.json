{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "components/*": ["./components/*"],
      "contexts/*": ["./contexts/*"],
      "hooks/*": ["./hooks/*"],
      "lib/*": ["./lib/*"],
      "pages/*": ["./pages/*"]
    },
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
